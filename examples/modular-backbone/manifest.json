{
	"name": "Smarter New Tab Page Beta",
	"description": "Automatically creates app icons for your most visited sites",

	"permissions": ["topSites", "<all_urls>", "storage"],
	
	"version": "0.1.1",
	"incognito": "split",
	"chrome_url_overrides": {
		"newtab": "index.html"
		},

	"manifest_version": 2,

	"content_security_policy": "script-src 'self'  https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'"


}
